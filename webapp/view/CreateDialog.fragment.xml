<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="createDialog" title="{dialog>/title}">
		<VBox class="sapUiResponsiveMargin">
			<Label text="{i18n>labelGroupID}" labelFor="Language" design="Bold"/>
			<Input  id="input" value="{detailView>/dialog/inputValue}"/>
		
		
		</VBox>
		<buttons>
			<Button type="Emphasized" text="{i18n>dialogSaveButtonText}"  press="onActionPress"/>
			<Button text="{i18n>dialogCancelButtonText}" press="onPressCancel"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>